  "dependencies": {
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "eslint-config-onny": "0.0.1-6",
    "mocha": "^6.2.3",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
