{
  "name": "onny-utils",
  "version": "1.0.6",
  "description": "Utils for Onny.io",
  "main": "./index.js",
  "scripts": {
    "lint": "eslint ./src --ext .js --format ./ node_modules/eslint-config-onny/eslintFormat",
    "lint-fix": "eslint ./src --ext .js --format ./node_modules/eslint-config-onny/eslintFormat --fix",
    "test_local": "mocha",
    "test": "nyc mocha && nyc report --reporter=text-lcov | coveralls",
    "git pull": "git pull https://github.com/imarotte/onny-utils",
    "git add": "git add ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onnyio/onny-utils.git"
  },
  "author": "Isaac Marotte <isaac@onny.io>",
  "contributors": [
    "Isaac Marotte <imarotte@onny.io>"
  ],
  "keywords": [
    "onny",
    "utils"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/onnyio/onny-utils/issues"
  },
  "homepage": "https://github.com/onnyio/onny-utils#readme",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "coveralls": "^2.13.0",
    "eslint-config-onny": "^1.0.1-0",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.2.0"
  }
}
