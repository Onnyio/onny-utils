{
  "name": "onny-utils",
  "version": "1.0.0-0",
  "description": "Utils for Onny.io",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "lint": "eslint ./src --ext .js --format ./node_modules/eslint-config-onny/eslintFormat",
    "lint-fix": "eslint ./src --ext .js --format ./node_modules/eslint-config-onny/eslintFormat --fix",
    "test": "mocha --require babel-polyfill --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onnyio/onny-utils.git"
  },
  "author": "Isaac Marotte <isaac@onny.io>",
  "contributors": [
    "Isaac Marotte <imarotte@onny.io>"
  ],
  "keywords": [
    "onny",
    "utils"
  ],
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/onnyio/onny-utils/issues"
  },
  "homepage": "https://github.com/onnyio/onny-utils#readme",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "eslint-config-onny": "^1.0.1-0",
    "mocha": "^3.2.0"
  }
}
